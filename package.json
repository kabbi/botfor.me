    "babel-core": "^6.3.17",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
